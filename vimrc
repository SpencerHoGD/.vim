 syntax on
 
 set number
 set nocompatible
 set showmode
 set relativenumber
 set cursorline
 set wrap
 set showcmd
 set wildmenu
 set mouse=a
 set encoding=utf-8
 set t_Co=256
 
 
 noremap S :w<CR>
 noremap Q :q<CR>
 noremap R :source $MYVIMRC<CR>
 
 filetype indent on

" noremap n h
" noremap u k
" noremap e j
" noremap i l
" 
" noremap k i
" noremap K I
" noremap l u
 
 
" map s <nop>
